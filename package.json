    "@angular/forms": "^14.0.2",
    "@angular/material": "^13.3.3",
    "@angular/platform-browser": "^14.0.2",
    "@angular/platform-browser-dynamic": "^14.0.2",
    "@angular/router": "^14.0.2",
    "@compodoc/compodoc": "^1.1.19",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "yt-player": "^3.5.0",
    "yt-player-angular": "^5.1.0",
    "zone.js": "~0.11.4"
