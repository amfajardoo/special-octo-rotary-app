{
  "name": "testing-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --base-href=/special-octo-rotary-app/",
    "audit": "npm audit fix --force",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --browsers ChromeHeadless",
    "test-code-coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "compodoc:build": "compodoc -p tsconfig.doc.json --theme material",
